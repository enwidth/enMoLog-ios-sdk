# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'RLLoggerSDKFramework' do
    # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
    use_frameworks!
    pod ‘Starscream’

    # Pods for RLLoggerSDKFramework
    
    target 'RLLoggerSDKFrameworkTests' do
        inherit! :search_paths
        pod ‘Starscream’
        # Pods for testing
    end
    
end

target 'RLLoggerSDKFramework_ObjC' do
    # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
    use_frameworks!
    
    # Pods for RLLoggerSDKFramework_ObjC
    pod ‘Starscream’
    
end

